{
    "manifest_version" : 2,
    "name" : "Dictionary",
    "version" : "0.1.0",
    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "js" : ["script.js"],
            "css" : ["style.css"]
        }
    ],
    "icons": {                   
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"  
      }, 
    "browser_action":{
        "default_icon": {                   
            "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"      
          },
        "default_popup": "popup.html",
        "default_title": "Embedded Dictionary for Searching Word Meanings"
    }
}